
# oh-my-zsh already aliases grep
# grep: aliased to grep  --color=auto --exclude-dir={.bzr,CVS,.git,.hg,.svn}

alias g='grep'
alias gi='grep -i'
alias gn='grep -n'
alias gr='grep -r'
alias gv='grep -v'
alias gin='grep -in'
alias grn='grep -rn'
alias grin='grep -rin'
alias gu='grep $(whoami)'
alias psg='psaux | g -i -e VSZ -e'

